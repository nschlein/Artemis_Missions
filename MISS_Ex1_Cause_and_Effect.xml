    <set_ship_text name="Derelict" newname="Kralien Cruiser" race="Kralien" class="Cruiser" desc="COMPUTER: It appears as though an enemy ship is in sector E5, but there are no life signs detected. Scan for further results with &quot;ENTER&quot; key." scan_desc="COMPUTER: An automated distress signal is active on the vessel. Life signs detected, but the energy levels are weak." hailtext="*distress signal*" />
  </event>
  <event name="Data Trail" id_arme="a090b077-1aa6-4afd-9676-1f3a1cc2e10c" parent_id_arme="94b41769-e58c-4827-a7d7-a3561f8482dc">
    <if_scan_level name="????" side="2" comparator="NOT" value="0" />
    <if_exists name="????" />
    <set_ship_text name="????" newname="Data Trail" desc="COMPUTER: Kralien comms signatures detected. To analyze further, scan with &quot;ENTER&quot; key." scan_desc="The Kralien vessel was spying on experiments at Tyken Station and sending vital TSN information to Hegemony leadership. It appears as though the Kralien fleet is on its to Earth. It is recommended that Excalibur proceeds to intercept point at Wolf 359." />
  </event>
  <folder_arme name="Weapons" id_arme="b278933e-b04f-4960-bb26-c5e0ba631b18" expanded_arme="" />
  <event name="Target Misdirection" id_arme="7227e40d-6dae-481e-a314-1b6bd0fd24e4" parent_id_arme="b278933e-b04f-4960-bb26-c5e0ba631b18">
