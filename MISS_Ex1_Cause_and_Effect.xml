    <set_ship_text name="??" newname="Temporal Distortion" desc="COMPUTER: Analyzing a distorted audio transmission coming from Tyken Station, but sensors indicate this broadcast should not even exist. More data required. Scan again with &quot;ENTER&quot; key." scan_desc="COMPUTER: Playing back best transmission quality. &quot;M--t -o-er d--- -----ds -it- E-- -o--e-o!&quot;" />
  </event>
  <event name="Magnetic Fissure" id_arme="1f91eb4e-5e46-4420-b082-16ace6505de8" parent_id_arme="94b41769-e58c-4827-a7d7-a3561f8482dc">
    <if_scan_level name="???" side="2" comparator="NOT" value="0" />
