  <folder_arme name="Comms Buttons" id_arme="5f4c0986-2881-4d43-a691-be6eb00e8fe7" expanded_arme="" />
  <event name="Comms Button I" id_arme="88dd303b-50a8-4dd4-9e75-da296e397172" parent_id_arme="5f4c0986-2881-4d43-a691-be6eb00e8fe7">
    <if_variable name="commsbutton" comparator="EQUALS" value="0.0" />
    <if_comms_button text="Tyken Station, this is Excalibur, how can we be of assistance?" />
    <incoming_comms_text from="Tyken Station" sideValue="2" type="PLAYER">Oh thank goodness TSN sent someone out here. We've encountered several malfunctions in key power distribution systems, and they've affected our long range comms, transporter functions, and most devastatingly our shield generators. Any assistance you can provide would be much appreciated.</incoming_comms_text>
    <set_timer name="commsbutton" seconds="30" />
    <set_variable name="commsbutton" value="1.0" integer="yes" />
    <clear_comms_button text="Tyken Station, this is Excalibur, how can we be of assistance?" sideValue="2" />
  </event>
  <event name="Comms Button II" id_arme="3a4b2d4a-18fe-4aad-bf99-5914664b3f29" parent_id_arme="5f4c0986-2881-4d43-a691-be6eb00e8fe7">
    <if_timer_finished name="commsbutton" />
    <if_variable name="commsbutton" comparator="EQUALS" value="1.0" />
    <clear_comms_button text="Tyken Station, this is Excalibur, how can we be of assistance?" sideValue="2" />
    <set_variable name="commsbutton" value="2.0" integer="yes" />
  </event>
  <folder_arme name="Helm" id_arme="b8480a2e-2eb0-410b-95af-da6a95beb42e" expanded_arme="" />
