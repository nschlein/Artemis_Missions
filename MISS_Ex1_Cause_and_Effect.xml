  <!--Mission Events-->
  <folder_arme name="Story Blurbs" id_arme="39a4ff97-d47e-43fd-8609-4e41411ef1a1" expanded_arme="" />
  <event name="TSN Command Briefing" id_arme="e74e270a-bb74-4b0f-b05e-4a78c08985b4" parent_id_arme="39a4ff97-d47e-43fd-8609-4e41411ef1a1">
    <if_variable name="causeandeffect" comparator="EQUALS" value="0.0" />
    <if_object_property property="countEMP" player_slot="0" comparator="EQUALS" value="4" />
    <if_timer_finished name="start_mission_timer_1" />
    <incoming_comms_text from="TSN COMMAND - STARDATE: 98657.98" sideValue="2" type="STATUS FRIEND">Excalibur, we believe that the USFP weapons reasearch station located in the Tyken Expanse is under attack. We are picking up Tyken Station's long range distress beacon, but communications are not getting through. You are the closest ship in range. You are to divert course from Wolf 359, and protect Tyken Station at all costs.</incoming_comms_text>
    <warning_popup_message message="COMPUTER: Review briefing on Comms." consoles="MHWESC" player_slot="0" />
    <set_variable name="causeandeffect" value="1.0" integer="yes" />
  </event>
  <event name="Sickbay Pages the Bridge" id_arme="1c127903-a01b-4fcc-9ed4-2bd026279293" parent_id_arme="39a4ff97-d47e-43fd-8609-4e41411ef1a1">
    <if_variable name="causeandeffect" comparator="EQUALS" value="1.0" />
    <if_timer_finished name="start_mission_timer_2" />
    <set_variable name="causeandeffect" value="2.0" integer="yes" />
    <incoming_comms_text from="SICKBAY" sideValue="2" type="STATUS">This is Sickbay to Bridge. A dozen members of our damage control teams just reported to sick bay with severe migraines, nausea, and intense feelings of deja vu all at once. It's probably nothing; however, I also can't help but feel as though I've treated them for these same symptoms before despite this being our first mission. Engineering is only going to have one team available for the time being. Sickbay out.</incoming_comms_text>
    <warning_popup_message message="COMPUTER: Sickbay on Comms" consoles="MC" player_slot="0" />
    <set_timer name="countdown_1" seconds="60" />
  </event>
