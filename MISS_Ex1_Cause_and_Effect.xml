    <if_exists name="Excalibur" />
    <if_player_is_targeting player_slot="0" name="Tyken Station" />
    <incoming_comms_text from="Tyken Station" sideValue="2" type="ALERT">What are you doing, Exalibur? Do not fire at us! We need your help! We are peaceful, not your enemy!</incoming_comms_text>
