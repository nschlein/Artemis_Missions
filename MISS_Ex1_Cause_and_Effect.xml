    <set_ship_text name="???" newname="Magnetic Fissure" desc="COMPUTER: Decrypting an unkown signal in Tyken Station's magnetic field. Results inconclusive. Scan again with &quot;ENTER&quot; key." scan_desc="COMPUTER: Translators read, &quot;/*/R/*/*/*/*/N/S/ /I/N/ /*/5/&quot;" />
  </event>
  <event name="Excalibur Warp Signature" id_arme="8b088d85-9a30-49c0-a0bd-2dc756459231" parent_id_arme="94b41769-e58c-4827-a7d7-a3561f8482dc">
    <if_scan_level name="Excalibur Warp Signature" side="2" comparator="NOT" value="0" />
    <if_exists name="Excalibur Warp Signature" />
    <set_ship_text name="Excalibur Warp Signature" newname="Excalibur Warp Signature" desc="COMPUTER: The Excalibur is in the same spot it was approximately 6 minutes ago, with almost everything the way it was before the shield generators overloaded and destroyed Tyken Station. Analyze our previous warp signature with the &quot;ENTER&quot; key." scan_desc="COMPUTER: The Excalibur has traveled back in time. It is recommended that the destruction of Tyken Station is prevented, otherwise this time loop may never end." />
  </event>
  <event name="Derelict" id_arme="6f29079b-9ad7-4ee3-9d0c-ed9a53670696" parent_id_arme="94b41769-e58c-4827-a7d7-a3561f8482dc">
