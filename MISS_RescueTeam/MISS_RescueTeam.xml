    <if_comms_button text="End Docking with Xi'Qin'Ah" />
    <warning_popup_message message="Disengaging docking." consoles="MHC" name="Artemis" />
    <set_variable name="docked" value="0.0" integer="yes" />
    <clear_comms_button text="End Docking with Xi'Qin'Ah" />
    <set_object_property property="topSpeed" value="0.6" name="Artemis" />
  </event>
  <event name="Transferring DC Teams" id_arme="297c9000-dc28-4115-93e8-fe284ed593fb">
    <if_variable name="scout_msg2" comparator="EQUALS" value="1.0" />
    <if_variable name="docked" comparator="EQUALS" value="1.0" />
    <if_variable name="damcon_transferred" comparator="EQUALS" value="0.0" />
    <warning_popup_message message="DamCon Teams 1 and 2 boarding the Xi'Qin'Ah." consoles="MHWESC" name="Artemis" />
    <set_damcon_members value="0.0" team_index="0" name="Artemis" />
    <set_damcon_members value="0.0" team_index="1" name="Artemis" />
    <set_variable name="damcon_transferred" value="1.0" integer="yes" />
    <!--!!!UPDATE:  Set to 180 seconds-->
    <set_timer name="Repair_scout" seconds="180" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">Thanks for docking with us.  Your DamCon teams are fixing us up.  We estimate it will take 3 minutes to repair our ship enough to jump out.</incoming_comms_text>
    <set_variable name="scout_msg2" value="0.0" integer="yes" />
    <set_timer name="energy_core" seconds="15" />
  </event>
  <event name="Energy Core Missing" id_arme="f0bb7db8-212f-480a-b922-891b4f8bcec5">
    <if_timer_finished name="energy_core" />
    <if_variable name="scout_msg5" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">During our emergency shutdown, we had to eject our HiDens Power Cell.  It should be around here in the asteroid field somewhere, if you need some extra power.</incoming_comms_text>
    <set_variable name="scout_msg5" value="0.0" integer="yes" />
  </event>
  <event name="Scout repaired" id_arme="d932474f-8efa-4339-b600-636bda460981">
    <if_timer_finished name="Repair_scout" />
    <if_variable name="scout_msg3" comparator="EQUALS" value="1.0" />
    <!---Scout repaired- -->
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">That did it, we're repaired enough to hop back to base.  Thanks.</incoming_comms_text>
    <set_object_property property="systemDamageBeam" value="0.0" name="Xi'Qin'Ah" />
    <set_object_property property="systemDamageImpulse" value="0.0" name="Xi'Qin'Ah" />
    <set_object_property property="systemDamageWarp" value="0.0" name="Xi'Qin'Ah" />
    <set_variable name="scout_broken" value="0.0" integer="yes" />
    <set_timer name="skaraan_appears" seconds="30" />
    <set_timer name="jump_start" seconds="45" />
    <set_variable name="scout_msg3" value="0.0" integer="yes" />
  </event>
  <event name="Return for DamCon" id_arme="12c79224-8b06-4f73-a0d8-a5f1034ec394">
    <if_variable name="damcon_transferred" comparator="EQUALS" value="1.0" />
    <if_variable name="scout_broken" comparator="EQUALS" value="0.0" />
    <if_variable name="docked" comparator="EQUALS" value="0.0" />
    <if_variable name="scout_msg6" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">You can return now to dock and retrieve your fix-it teams.  Thanks again for the better-making.</incoming_comms_text>
    <set_variable name="scout_msg6" value="0.0" integer="yes" />
  </event>
  <event name="Waiting for DamCon" id_arme="b8524dd2-c824-4c4a-9233-7eea651e0065">
    <if_variable name="damcon_transferred" comparator="EQUALS" value="1.0" />
    <set_object_property property="throttle" value="0.0" name="Xi'Qin'Ah" />
  </event>
  <event name="Return DamCon" id_arme="eaa1c267-2be9-4f1b-9e3a-a998501da0b5">
    <if_variable name="damcon_transferred" comparator="EQUALS" value="1.0" />
    <if_variable name="docked" comparator="EQUALS" value="1.0" />
    <if_variable name="scout_broken" comparator="EQUALS" value="0.0" />
    <warning_popup_message message="DamCon Teams 1 and 2 returning from Xi'Qin'Ah." consoles="MHWESC" name="Artemis" />
    <set_damcon_members value="6" team_index="0" name="Artemis" />
    <set_damcon_members value="6" team_index="1" name="Artemis" />
    <set_variable name="damcon_transferred" value="0.0" integer="yes" />
  </event>
  <event name="Skaraan Appears" id_arme="7aefd213-e2e8-46f3-a551-8a80b7b00292">
    <if_timer_finished name="skaraan_appears" />
    <if_variable name="hunter_appears" comparator="EQUALS" value="0.0" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">Wait!  That's the same Executor that rudely attacked us earlier!  It must have been hiding in the nebula this whole time.  Can you go smack it around for us?  We did manage to hit it with a Tag -- hopefully that will help you to find it if it cloaks again.</incoming_comms_text>
    <set_variable name="hunter_appears" value="1.0" integer="yes" />
    <create type="enemy" x="44814.0" y="0.0" z="27367.0" angle="0" name="Hunter946" hullID="5002" sideValue="1" fleetnumber="-1" />
    <clear_ai name="Hunter946" />
    <add_ai type="ATTACK" targetName="Command528" value1="1.0" name="Hunter946" />
    <add_ai type="CHASE_PLAYER" value1="6000" value2="3000" name="Hunter946" />
    <set_object_property property="isTagged" value="1" name="Hunter946" />
    <set_named_object_tag_state name="Hunter946" tagged="1" tagSourceName="Xi'Qin'Ah" tagSourceSide="2" />
    <set_special name="Hunter946" clear="yes" ability="AntiTorp" />
    <set_special name="Hunter946" ability="Warp" />
    <set_special name="Hunter946" ability="Drones" />
    <set_special name="Hunter946" ship="1" captain="1" ability="Cloak" />
  </event>
  <event name="Scout jump start wDC" id_arme="446551d8-3288-422a-9c28-4c6f91500c86">
    <if_timer_finished name="jump_start" />
    <if_variable name="damcon_transferred" comparator="EQUALS" value="1.0" />
    <if_variable name="scout_msg7" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">We're too damaged to fight.  We're going to hop back to base for full repairs.  We'll stick around for 2 minutes to give you a chance to pick up your fix-it teams, otherwise we'll bring them back to base with us to keep them safe.  We'll also transfer our last two tracking torpedoes to help you out.</incoming_comms_text>
    <addto_object_property value="2.0" property="countHoming" name="Artemis" />
    <set_object_property property="throttle" value="50.0" name="Xi'Qin'Ah" />
    <set_timer name="jump_prep" seconds="120" />
    <set_variable name="scout_msg7" value="0.0" integer="yes" />
  </event>
  <event name="Scout jump start woDC" id_arme="1229328c-1dd5-43e3-a3e4-0b4a69fe8ba2">
    <if_timer_finished name="jump_start" />
    <if_variable name="damcon_transferred" comparator="EQUALS" value="0.0" />
    <if_variable name="scout_msg7" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">We're too damaged to fight.  We're going to hop back to base for full repairs.  We'll transfer our last two tracking torpedoes to help you out.</incoming_comms_text>
    <addto_object_property value="2.0" property="countHoming" name="Artemis" />
    <set_object_property property="throttle" value="50.0" name="Xi'Qin'Ah" />
    <set_timer name="jump_prep" seconds="30" />
    <set_variable name="scout_msg7" value="0.0" integer="yes" />
  </event>
  <event name="Scout jumped" id_arme="f2a34764-e6b5-4321-b3b7-dcaddfa4a7f4">
    <if_timer_finished name="jump_prep" />
    <if_variable name="scout_jumped" comparator="EQUALS" value="1.0" />
    <if_variable name="docked" comparator="EQUALS" value="0.0" />
    <set_relative_position name2="Xi'Qin'Ah" distance="100" angle="0" name1="Command528" />
    <clear_ai name="Xi'Qin'Ah" />
    <add_ai type="GUARD_STATION" value1="7000" name="Xi'Qin'Ah" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">We've jumped back to base.  We'll repair here and stick around in case it gets attacked.</incoming_comms_text>
    <set_variable name="scout_jumped" value="0.0" integer="yes" />
    <set_variable name="damcon_transferred" value="0.0" integer="yes" />
  </event>
  <event name="Begin End Game" id_arme="19ce7654-b4e5-4860-a59f-1f687be001cc">
    <if_variable name="hunter_appears" comparator="EQUALS" value="1.0" />
    <if_not_exists name="Hunter946" />
    <if_variable name="scout_msg4" comparator="EQUALS" value="1.0" />
    <set_timer name="end_game" seconds="60" />
    <incoming_comms_text from="Xi'Qin'Ah" sideValue="2" type="FRIEND">Oh, I see, they used their cloaking device.  My mind must be buggled.</incoming_comms_text>
    <incoming_comms_text from="Command528" sideValue="2" type="STATION">Great job, Rescue Team!  You saved the Ximni Scout and defeated the Skaraan.  Nice work.</incoming_comms_text>
    <set_variable name="scout_msg4" value="0.0" integer="yes" />
  </event>
  <event name="Finish End Game" id_arme="a0296bd9-a134-42f9-9984-0bb6f4ed29ca">
    <if_timer_finished name="end_game" />
    <end_mission />
  </event>
